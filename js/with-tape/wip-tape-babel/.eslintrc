{
  "parser": "babel-eslint",
  "env": {
    "browser": false,
    "node": true
  },
  "globals": {
    "console": true
  },
  "rules": {
    "eqeqeq": 2,
    "curly": 2,
    "quotes": [ 2, "single" ],
    "camelcase": 2,
    "key-spacing": 2,
    "new-cap": 2,
    "no-trailing-spaces": 2,
    "no-unreachable": 2,
    "no-console": 0,
    "no-use-before-define": 0,
    "no-extra-strict": 0,
    "space-after-keywords": 2,
    "no-var": 1,
    "semi": [ 2, "always" ],
    "dot-notation": [ 2, { "allowKeywords": false } ]
  }
}
